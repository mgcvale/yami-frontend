		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.4",
		"typescript": "^5.0.0",
		"vite": "^6.2.5",
		"vite-plugin-pwa": "^1.0.0"
	},
	"dependencies": {
		"@capacitor/android": "^7.2.0",
		"@capacitor/cli": "^7.2.0",
		"@capacitor/core": "^7.2.0",
		"@lucide/svelte": "^0.488.0",
